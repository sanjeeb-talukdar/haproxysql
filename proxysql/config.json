{
  "datacenter": "proxysql-sandbox",
  "data_dir": "/tmp/consul",
  "log_level": "warn",
  "enable_local_script_checks": true,
  "disable_update_check": true,
  "encrypt": "X4SYOinf2pTAcAHRhpj7dA==",
  "leave_on_terminate": true,
  "node_meta":
  {
    "service": "proxysql"
  },
  "retry_join":
  [
    "172.16.238.150"
  ],
  "services":[
    {
      "id":"proxysql",
      "name":"proxysql",
	  "checks": [
        {
          "id":"port",
          "name":"Listen on port 6032",
          "tcp":"localhost:6032",
          "interval":"1s",
          "timeout":"100ms"
        }
      ]
    }
  ],
  "watches": [
    {
      "type": "service",
      "service": "mysql",
      "args": ["/root/mysql-handler.sh"]
    },
	{
      "type": "service",
      "service": "proxysql",
      "args": ["/root/proxysql-handler.sh"]
    }
  ]
}
